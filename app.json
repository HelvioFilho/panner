{
  "expo": {
    "name": "planner",
    "slug": "planner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/main/icon.png",
    "scheme": "planner",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/main/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hsvf.planner"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/main/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hsvf.planner"
    },
    "plugins": ["expo-router", "expo-font"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
